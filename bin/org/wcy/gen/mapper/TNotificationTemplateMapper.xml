<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.wcy.gen.mapper.TNotificationTemplateMapper" >

    <resultMap id="BaseResultMap" type="org.wcy.gen.model.TNotificationTemplate" >
        <result column="ID" property="id" jdbcType="DECIMAL" />
        <result column="NAME" property="name" jdbcType="VARCHAR" />
        <result column="SYSTEM" property="system" jdbcType="VARCHAR" />
        <result column="TITLE" property="title" jdbcType="VARCHAR" />
        <result column="TEMPLATE" property="template" jdbcType="VARCHAR" />
        <result column="DELETED" property="deleted" jdbcType="DECIMAL" />
    </resultMap>

    <insert id="insert" parameterType="org.wcy.gen.model.TNotificationTemplate" >
        insert into T_NOTIFICATION_TEMPLATE (ID, NAME, SYSTEM, 
            TITLE, TEMPLATE, DELETED
            )
        values (#{id,jdbcType=DECIMAL}, #{name,jdbcType=VARCHAR}, #{system,jdbcType=VARCHAR}, 
            #{title,jdbcType=VARCHAR}, #{template,jdbcType=VARCHAR}, #{deleted,jdbcType=DECIMAL}
            )
    </insert>

    <insert id="insertSelective" parameterType="org.wcy.gen.model.TNotificationTemplate" >
        insert into T_NOTIFICATION_TEMPLATE
        <trim prefix="(" suffix=")" suffixOverrides="," >
            <if test="id != null" >
                ID,
            </if>
            <if test="name != null" >
                NAME,
            </if>
            <if test="system != null" >
                SYSTEM,
            </if>
            <if test="title != null" >
                TITLE,
            </if>
            <if test="template != null" >
                TEMPLATE,
            </if>
            <if test="deleted != null" >
                DELETED,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides="," >
            <if test="id != null" >
                #{id,jdbcType=DECIMAL},
            </if>
            <if test="name != null" >
                #{name,jdbcType=VARCHAR},
            </if>
            <if test="system != null" >
                #{system,jdbcType=VARCHAR},
            </if>
            <if test="title != null" >
                #{title,jdbcType=VARCHAR},
            </if>
            <if test="template != null" >
                #{template,jdbcType=VARCHAR},
            </if>
            <if test="deleted != null" >
                #{deleted,jdbcType=DECIMAL},
            </if>
        </trim>
    </insert>
</mapper>